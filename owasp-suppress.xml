<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for Azure SDK dependencies -->
    <suppress>
        <notes>Azure SDK dependencies are regularly updated and monitored by Microsoft</notes>
        <packageUrl regex="true">^pkg:maven/com\.azure/.*$</packageUrl>
    </suppress>
    
    <!-- Suppress false positives for Keycloak dependencies -->
    <suppress>
        <notes>Keycloak dependencies are provided by the runtime environment</notes>
        <packageUrl regex="true">^pkg:maven/org\.keycloak/.*$</packageUrl>
    </suppress>
    
    <!-- Suppress false positives for test-only dependencies -->
    <suppress>
        <notes>Test-only dependencies are not part of the production runtime</notes>
        <packageUrl regex="true">^pkg:maven/org\.testcontainers/.*$</packageUrl>
    </suppress>
    
    <!-- Suppress false positives for SLF4J which is provided by Keycloak -->
    <suppress>
        <notes>SLF4J is provided by the Keycloak runtime</notes>
        <packageUrl regex="true">^pkg:maven/org\.slf4j/.*$</packageUrl>
    </suppress>
</suppressions>