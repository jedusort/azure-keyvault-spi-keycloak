<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- CVE-2023-36415 False Positive Suppression -->
    <suppress>
        <notes>CVE-2023-36415: False positive - fixed in azure-identity 1.16.1+</notes>
        <packageUrl regex="true">^pkg:maven/com\.azure/azure-identity@1\.16\.[2-9].*$</packageUrl>
        <cve>CVE-2023-36415</cve>
    </suppress>
    
    <!-- Suppress all Keycloak dependencies as they are provided by runtime -->
    <suppress>
        <notes>Keycloak dependencies are provided by the runtime environment</notes>
        <packageUrl regex="true">^pkg:maven/org\.keycloak/.*$</packageUrl>
        <cve>CVE-2024-11736</cve>
    </suppress>
    
